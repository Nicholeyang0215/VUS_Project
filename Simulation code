res1 = read.csv("result.csv")  
res2 = read.csv("1m.csv")  ## 45 replicates for sample size = 1 million


## get sample size of the data
n1 = nrow(res1)
n2 = nrow(res2)

### Calculate mean
resMean = apply(res1, 2, mean)

### Calculate CI
resLo = apply(res1, 2, function(x) quantile(x, 0.025))
resUp = apply(res1, 2, function(x) quantile(x, 0.975))
